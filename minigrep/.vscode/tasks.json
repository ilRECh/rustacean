{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--",
				"Are",
				"poem.txt"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run",
			"group": {
				"kind": "build",
				"isDefault": true,
			},
		},

		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo test",
			"group": {
				"kind": "build",
				"isDefault": false,
			},
		}
	]
}